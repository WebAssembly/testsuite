    (type $d (sub $b (describes $c) (descriptor $e) (struct)))
    (type $e (describes $d) (struct))
  )

  ;; Cast to self
  (func (param (ref null $a) (ref null $b)) (result (ref $a))
    (block (result (ref null $a))
      (br_on_cast_desc_fail 1 (ref null $a) (ref null $a)
        (local.get 0)
        (local.get 1)
