        (local.get 1)
      )
    )
    (unreachable)
  )

  ;; Cast from defined type to unrelated type
  (func (param (ref null $e) (ref null $b)) (result (ref $e))
    (block (result (ref null $a))
      (br_on_cast_desc_fail 1 (ref null $e) (ref null $a)
